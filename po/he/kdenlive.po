#: src/dialogs/subtitleedit.cpp:251
#, kde-format
msgid "Edit subtitle style"
msgstr "עריכת סגנון כתוביות"

#: src/dialogs/subtitleedit.cpp:252
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid "Opens a dialog to edit the current subtitle."
msgstr "פתיחת חלונית לעריכת הכתוביות הנוכחיות."

#: src/dialogs/subtitleedit.cpp:279
#, kde-format
msgid "Go to next subtitle"
msgstr "מעבר לכתובית הבאה"

#: src/dialogs/subtitleedit.cpp:280
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Moves the playhead in the timeline to the beginning of the subtitle to the "
"right."
msgstr "מעביר את ראש הנגינה בציר הזמן לתחילת הכתובית מימין."

#: src/dialogs/subtitleedit.cpp:281
#, kde-format
msgid "Go to previous subtitle"
msgstr "מעבר לכתובית הקודמת"

#: src/dialogs/subtitleedit.cpp:282
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Moves the playhead in the timeline to the beginning of the subtitle to the "
"left."
msgstr "מעביר את ראש הנגינה בציר הזמן לתחילת הכתובית משמאל."

#: src/dialogs/subtitleedit.cpp:284
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Creates a new subtitle with the default length (set in <interface>Settings-"
">Configure Kdenlive…->Misc</interface>) at the current playhead position/"
"frame."
msgstr ""
"יצירת כתובית חדשה באורך ברירת המחדל (מוגדר תחת <interface>הגדרות->הגדרת "
"Kdenlive…->שונות</interface>) בתמונית/מקום הנוכחיים של ראש הנגינה."

#: src/dialogs/subtitleedit.cpp:286
#, kde-format
msgid "Split subtitle at cursor position"
msgstr "פיצול הכתובית במקום של הסמן"

#: src/dialogs/subtitleedit.cpp:288
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Cuts the subtitle text at the cursor position and creates a new subtitle to "
"the right (like cutting a clip)."
msgstr ""
"גזירת טקסט הכתוביות במקום של הסמן ויצירת כתובית חדשה מימין (כמו חיתוך של "
"סרטון)."

#: src/dialogs/subtitleedit.cpp:289
#, kde-format
msgid "Update subtitle text"
msgstr "עדכון טקסט הכתובית"

#: src/dialogs/subtitleedit.cpp:290
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid "Updates the subtitle display in the timeline."
msgstr "עדכון תצוגת הכתוביות בציר הזמן."

#: src/dialogs/subtitleedit.cpp:291
#, kde-format
msgid "Show more options"
msgstr "הצגת אפשרויות נוספות"

#: src/dialogs/subtitleedit.cpp:292
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid "Toggles a list to adjust more subtitle settings."
msgstr "הצגה/הסתרה של רשימה להתאמת הגדרות כתוביות נוספות."

#: src/dialogs/subtitleedit.cpp:294
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Deletes the currently selected subtitle (doesn’t work on multiple subtitles)."
msgstr "מוחק את הכתובית הנבחרת (לא עובד עם מגוון כתוביות)."

#: src/dialogs/subtitleedit.cpp:297
#, kde-format
msgid "Toggle bold"
msgstr "הפעלה/השבתה של הדגשה"

#: src/dialogs/subtitleedit.cpp:298
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Toggles the bold text style for either the selected text or the text "
"following the cursor position."
msgstr ""
"החלפת סגנון הטקסט המודגש לקטע הטקסט שנבחר או לטקסט שנמצא מיד אחרי מקום הסמן."

#: src/dialogs/subtitleedit.cpp:301
#, kde-format
msgid "Toggle italic"
msgstr "הפעלה/השבתה של הטיה"

#: src/dialogs/subtitleedit.cpp:303
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Toggles the italic text style for either the selected text or the text "
"following the cursor position."
msgstr ""
"החלפת סגנון הטקסט הנטוי לקטע הטקסט שנבחר או לטקסט שנמצא מיד אחרי מקום הסמן."

#: src/dialogs/subtitleedit.cpp:306
#, kde-format
msgid "Toggle underline"
msgstr "הפעלה/השבתה של קו תחתי"

#: src/dialogs/subtitleedit.cpp:308
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Toggles the underline text style for either the selected text or the text "
"following the cursor position."
msgstr ""
"החלפת סגנון הטקסט עם הקו התחתי לקטע הטקסט שנבחר או לטקסט שנמצא מיד אחרי מקום "
"הסמן."

#: src/dialogs/subtitleedit.cpp:311
#, kde-format
msgid "Toggle strikeout"
msgstr "הפעלה/השבתה של קו חוצה"

#: src/dialogs/subtitleedit.cpp:313
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Toggles the strikeout text style for either the selected text or the text "
"following the cursor position."
msgstr ""
"החלפת סגנון הטקסט עם הקו החוצה לקטע הטקסט שנבחר או לטקסט שנמצא מיד אחרי מקום "
"הסמן."

#: src/dialogs/subtitleedit.cpp:318
#, kde-format
msgid "Set font"
msgstr "הגדרת גופן"

#: src/dialogs/subtitleedit.cpp:321
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Open a font dialog to select a font and set its properties for either the "
"selected text or the text following the cursor position."
msgstr ""
"פתיחת חלונית גופנים כדי לבחור גופן ולהגדיר את מאפייניו או לטקסט הנבחר או "
"לטקסט שנמצא אחרי מקום הסמן."

#: src/dialogs/subtitleedit.cpp:324
#, kde-format
msgid "Reset style"
msgstr "איפוס סגנון"

#: src/dialogs/subtitleedit.cpp:325
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"Resets the style of either the selected text or the text following the "
"cursor position."
msgstr "איפוס הסגנון של הטקסט הנבחר או של הטקסט שנמצא אחרי מקום הסמן."

#: src/dialogs/subtitleedit.cpp:327
#, kde-format
