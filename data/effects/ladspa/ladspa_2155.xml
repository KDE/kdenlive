<?xml version="1.0"?>
<!DOCTYPE kpartgui>
<effect xmlns="https://www.kdenlive.org" id="ladspa.2155" tag="ladspa.2155" ladspaid="2155" type="audio">
   <name>TAP Pink/Fractal Noise</name>
   <description><![CDATA[TAP Pink/Fractal Noise (LADSPA 2155)<br>
    Adds pink noise to the incoming signal using a one-dimensional random fractal line generated by the Midpoint Displacement Method.]]></description>
   <author>http://www.ladspa.org</author>
   <parameter name="0" type="animated" decimals="3" default="0.500" max="1.000" min="0.000">
      <name>Fractal Dimension</name>
   </parameter>
   <parameter name="1" type="animated" decimals="1" default="0.0" max="20.0" min="-90.0" suffix="dB">
      <name>Signal Level</name>
   </parameter>
   <parameter name="2" type="animated" decimals="1" default="-90.0" max="20.0" min="-90.0" suffix="dB">
      <name>Noise Level</name>
   </parameter>
   <parameter name="wetness" type="animated" decimals="0" default="1" max="100" min="0" factor="100">
      <name>Dry/Wet</name>
   </parameter>
</effect>
