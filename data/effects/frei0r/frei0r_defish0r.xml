<?xml version="1.0"?>
<!DOCTYPE kpartgui>
<effect xmlns="https://www.kdenlive.org" LC_NUMERIC="C" tag="frei0r.defish0r" id="frei0r.defish0r">
    <name context="Defish Effect Name">Defish</name>
    <description>Non rectilinear lens mappings</description>
    <author>Marko Cebokli</author>
    <parameter type="animated" name="Amount" default="0" min="0" max="1000" factor="1000">
        <name>Amount</name>
    </parameter>
    <parameter type="bool" name="DeFish" default="1">
        <name>De-Fish</name>
    </parameter>
    <parameter type="list" name="Type" default="0.666" paramlist="0;0.333;0.666;1">
        <name>Type</name>
        <paramlistdisplay>Equidistant,Orthographic,Equiarea,Stereographic</paramlistdisplay>
    </parameter>
    <parameter type="list" name="Scaling" default="0" paramlist="0;0.333;0.666;1">
        <name>Scaling</name>
        <paramlistdisplay>Fill,Center,Fit,Manual</paramlistdisplay>
    </parameter>
    <parameter type="bool" name="Crop" default="0">
        <name><![CDATA[Straighten all Edges<br>
        of Video Frame]]></name>
    </parameter>
    <parameter type="animated" name="Non-Linear scale" max="1000" min="0" factor="1000" default="0.5">
        <name><![CDATA[Fix Camera Scaling<br>
        Between 4:3 and 16:9]]></name>
    </parameter>
    <parameter type="animated" name="Y Scale" max="1000" min="0" factor="1000" default="0.5">
        <name><![CDATA[Scale Y to Affect<br>
        Aspect Ratio]]></name>
    </parameter>
    <parameter type="animated" name="Manual Scale" default="0.5" min="0" max="1000" factor="1000">
        <name>Manual Scale</name>
    </parameter>
    <parameter type="list" name="Interpolator" default="0.166" paramlist="0;0.166;0.333;0.5;0.666;0.833;1">
        <name>Interpolator</name>
        <paramlistdisplay>Nearest neighbor,Bilinear,Bicubic smooth,Bicubic sharp,Spline 4x4,Spline 6x6,Lanczos</paramlistdisplay>
    </parameter>
    <parameter type="list" name="Aspect type" default="0" paramlist="0;0.25;0.5;0.75;1">
        <name>Aspect Type</name>
        <paramlistdisplay>Square,PAL DV,NTSC DV,HDV,Manual</paramlistdisplay>
    </parameter>
    <parameter type="animated" name="Manual Aspect" default="0.5" min="0" max="1000" factor="1000">
        <name>Manual Aspect</name>
    </parameter>
</effect>
