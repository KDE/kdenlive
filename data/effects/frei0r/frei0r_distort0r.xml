<?xml version="1.0"?>
<!DOCTYPE kpartgui>
<effect xmlns="https://www.kdenlive.org" tag="frei0r.distort0r" id="frei0r.distort0r">
    <name context="Distort Effect Name">Distort</name>
    <description>Creates a plasma like distortion</description>
    <author>Gephex crew</author>
    <parameter type="animated" name="Amplitude" max="100" min="0" default="0.5" factor="100" decimals="1" suffix="%">
        <name>Amplitude</name>
    </parameter>
    <parameter type="animated" name="Frequency" max="100" min="0" default="0.005" factor="100" decimals="1" suffix="%">
        <name>Frequency</name>
    </parameter>
    <parameter type="bool" name="Use Velocity" max="1" min="0" default="1">
        <name>Use Velocity</name>
    </parameter>
    <parameter type="animated" name="Velocity" max="100" min="0" default="0.5" factor="100" decimals="1" suffix="%">
        <name>Velocity</name>
    </parameter>
</effect>
