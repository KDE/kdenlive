<!DOCTYPE kpartgui>
<effect tag="sox" id="sox_reverb" type="audio">
	<name>Sox Reverb</name>
	<description>Sox reverb audio effect</description>
	<author>http://sox.sourceforge.net</author>
	<parameter type="constant" name="reverberance" max="100" min="0" default="50" suffix="%">
		<name>Reverberance</name>
	</parameter>
	<parameter type="constant" name="hf_damping" max="100" min="0" default="50" suffix="%">
		<name>High frequency damping</name>
	</parameter>	
	<parameter type="constant" name="room_scale" max="100" min="0" default="100" suffix="%">
		<name>Room scale</name>
	</parameter>
	<parameter type="constant" name="stereo_depth" max="100" min="0" default="100" suffix="%">
		<name>Stereo depth</name>
	</parameter>
	<parameter type="constant" name="pre_delay_ms" max="500" min="0" default="0" suffix="ms">
		<name>Pre-delay</name>
	</parameter>
	<parameter type="constant" name="wet_gain_dB" max="10" min="-10" default="0" suffix="dB">
		<name>Wet gain</name>
	</parameter>
</effect>
