<!DOCTYPE kpartgui>
<effect tag="wave" id="wave">
	<name>Wave</name>
	<description>Make waves on your clip with keyframes</description>
	<author>Leny Grisel</author>
	<parameter type="animated" name="start" max="100" min="0" default="5">
		<name>Amplitude</name>
	</parameter>
	<parameter type="bool" name="deformX" default="1">
		<name>Horizontal</name>
	</parameter>
	<parameter type="bool" name="deformY" default="0">
		<name>Vertical</name>
	</parameter>
</effect>
